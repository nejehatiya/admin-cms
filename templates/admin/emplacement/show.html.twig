{% extends 'base.html.twig' %}

{% block title %}Emplacement{% endblock %}

{% block body %}
    <h1>Emplacement</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ emplacement.id }}</td>
            </tr>
            <tr>
                <th>Key_emplacement</th>
                <td>{{ emplacement.keyEmplacement }}</td>
            </tr>
            <tr>
                <th>Status</th>
                <td>{{ emplacement.status ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Date_add</th>
                <td>{{ emplacement.dateAdd ? emplacement.dateAdd|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Date_upd</th>
                <td>{{ emplacement.dateUpd ? emplacement.dateUpd|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_emplacement_index') }}">back to list</a>

    <a href="{{ path('app_emplacement_edit', {'id': emplacement.id}) }}">edit</a>

    {{ include('emplacement/_delete_form.html.twig') }}
{% endblock %}
