{% extends 'base.html.twig' %}

{% block title %}PostMetaFields{% endblock %}

{% block body %}
    <h1>PostMetaFields</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ post_meta_field.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ post_meta_field.name }}</td>
            </tr>
            <tr>
                <th>Fields</th>
                <td>{{ post_meta_field.fields }}</td>
