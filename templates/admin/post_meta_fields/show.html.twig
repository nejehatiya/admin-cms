            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_post_meta_fields_index') }}">back to list</a>

    <a href="{{ path('app_post_meta_fields_edit', {'id': post_meta_field.id}) }}">edit</a>

    {{ include('post_meta_fields/_delete_form.html.twig') }}
